<!DOCTYPE html>
<html lang="es-es">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>SmashList - Smart Shopping List</title>
    <!-- base:css -->
    <link rel="stylesheet" href="~/Scripts/vendors/typicons/typicons.css">
    <link rel="stylesheet" href="~/Scripts/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Scripts/css/vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Scripts/images/favicon.png" />

    @*<link rel="stylesheet" href="//cdn.datatables.net/1.11.5/css/jquery.dataTables.min.css">*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Scripts/izi-toast/css/iziToast.min.css">

    <link rel="stylesheet" href="~/css/site.css">

    <!-- JQUERY -->
    <script src="~/Scripts/js/jquery-3.6.0.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>
<body>
    <!-- HEADER -->
    <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
        <div class="navbar-brand-wrapper d-flex justify-content-center">
            <div class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100">
                <a class="navbar-brand brand-logo" href="index.html"><img src="~/Scripts/images/logo.svg" alt="logo" /></a>
                <a class="navbar-brand brand-logo-mini" href="index.html"><img src="~/Scripts/images/logo-mini.svg" alt="logo" /></a>
                <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                    <span class="typcn typcn-th-menu"></span>
                </button>
            </div>
        </div>
        <div class="navbar-menu-wrapper d-flex align-items-center justify-content-center">
            <ul class="navbar-nav">
                <li class="nav-item ml-0">
                    <h4 class="mb-0">@ViewData["Title"]</h4>
                </li>
                @*<li class="nav-item">
                        <div class="d-flex align-items-baseline">
                            <p class="mb-0">Home</p>
                            <i class="typcn typcn-chevron-right"></i>
                            <p class="mb-0">Main Dahboard</p>
                        </div>
                    </li>*@
            </ul>

            <ul class="navbar-nav navbar-nav-right">
                <li class="nav-item nav-search d-none d-md-block mr-0">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search..." aria-label="search" aria-describedby="search">
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="search">
                                <i class="typcn typcn-zoom"></i>
                            </span>
                        </div>
                    </div>
                </li>
            </ul>
            <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                <span class="typcn typcn-th-menu"></span>
            </button>
        </div>
    </nav>

    <!-- CONTAINER PRINCIPAL -->
    <div class="container-fluid page-body-wrapper">
        <!-- MENU LATERAL -->
        <partial name="~/Views/Shared/_MenuLateral.cshtml" />

        <!-- START PAGE -->
        <div class="main-panel">
            <div class="content-wrapper">
                @RenderBody()
            </div>
            <!-- FOOTER -->
            <footer class="footer">
                <div class="card">
                    <div class="card-body">
                        <div class="d-sm-flex justify-content-center justify-content-sm-between">
                            <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © 2020 <a href="https://www.bootstrapdash.com/" class="text-muted" target="_blank">Elena Agudo</a>. All rights reserved.</span>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <!-- base:js -->
    <script src="~/Scripts/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->

    @RenderSection("Scripts", required: false)

    <!-- inject:js -->
    <script src="~/Scripts/js/off-canvas.js"></script>
    <script src="~/Scripts/js/hoverable-collapse.js"></script>
    
    <!-- 20-04-2022 Modificación Elena -->
    <!-- Este archivo js se ha modificado (se han comentado algunas partes que hacian que el menu no funcionara como quiero) -->
    <script src="~/Scripts/js/template.js"></script>
    @*<script src="~/Scripts/js/settings.js"></script>
    <script src="~/Scripts/js/todolist.js"></script>*@
    <!-- endinject -->

</body>
</html>

